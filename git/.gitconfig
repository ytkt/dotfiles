[user]
        name = ytkt
        email = yyttkktt@gmail.com
	signingkey = A76FA1B3
[color]
        ui = auto
[alias]
  ch = checkout
  co = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
[core]
        excludesfile = /Users/yuta/.gitignore
        editor = "emacs -q"
[difftool "sourcetree"]
        cmd = opendiff \"$LOCAL\" \"$REMOTE\"
        path =
[mergetool "sourcetree"]
        cmd = /Applications/SourceTree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
        trustExitCode = true
[gpg]
	program = gpg2
[commit]
	gpgsign = false
